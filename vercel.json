{
    "version": 2,
    "builds":[{
        "src": "*.js",
        "use": "@vercel/node"
    }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
          "default": {
            "load": { "maxDuration": 30 }
          }
        }
      
}